{
	"manifest_version" : 2,
	"name":"Qrcode",
	"description":"Hunter",
	"version":"1.0",
	"browser_action":{
		"default_icon":"d.png",
		"default_title":"DH",
		"default_popup":"popup.html"
	},
	"content_scripts":[
		{
			"matches":["http://*/*"],
			"js":["jquery.js","hunter.js"],
			"run_at":"document_end"
		}
	],
	"permissions":[
		"tabs",
		"notifications",
		"http://*/*",
		"activeTab"
	],
	"optional_permissions":[
		"tabs"
	]
}
